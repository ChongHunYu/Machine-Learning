    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "N = 4\n",
    "x = np.arange(-3,3,0.01)\n",
    "y = np.sin(x)\n",
    "y2 = x\n",
    "y3 = x - x**3/(3*2)\n",
    "y4 = x - x**3/3 + x**5/(5*4*3*2)\n",
    "y5 = x - x**3/3 + x**5/(5*4*3*2) - x**7/(7*6*5*4*3*2)\n",
    "plt.plot(x,y)\n",
    "plt.plot(x,y2)\n",
    "plt.plot(x,y3)\n",
    "plt.plot(x,y4)\n",
    "plt.plot(x,y5)\n",
    "plt.legend(['sin','one','two','three','four','five'])\n",
