   ],
   "source": [
    "def sigmoid(array):\n",
    "    \"\"\"Applies the sigmoid or logistic function to a numpy array\"\"\"\n",
    "    return 1/(1+np.exp(-array))\n",
    "\n",
    "\n",
    "xmin = np.min(X[:,1])-0.5\n",
    "xmax = np.max(X[:,1])+0.5\n",
    "ymin = np.min(X[:,2])-0.5\n",
    "ymax = np.max(X[:,2])+0.5\n",
    "x1 = np.arange(xmin,xmax,0.01)\n",
    "x2 = np.arange(ymin,ymax,0.01)\n",
    "\n",
    "x1v,x2v = np.meshgrid(x1,x2)\n",
    "\n",
    "varz = [np.ones_like(x1v),x1v,x2v,x1v**2,x2v**2,x1v*x2v,x1v**3,x2v**3,x1v**4,x2v**4,x1v**2*x2v**2]\n",
    "\n",
    "z = np.zeros_like(x1v)\n",
    "for ww,v in zip(w,varz):\n",
    "    z += ww*v\n",
    "\n",
    "zsoft = sigmoid(z)\n",
    "#z = np.sign(z)\n",
    "plt.contourf(x1v,x2v,zsoft,alpha=0.25)\n",
    "Xg = X[y==1]\n",
    "Xb = X[y==-1]\n",
    "\n",
    "plt.scatter(Xg[:,1],Xg[:,2],c=\"b\",alpha=0.5)\n",
    "plt.scatter(Xb[:,1],Xb[:,2],c='r',alpha=0.5)\n",
    "plt.xlabel(r\"$x_1$\")\n",
    "plt.ylabel(r\"$x_2$\")\n",
    "\n",
    "plt.title(\"Contour plot of soft boundary\")\n",
    "plt.colorbar()\n",
    "plt.show()\n"
   ]
  },
