    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "xx = np.linspace(-7,7)\n",
    "yy = np.exp(xx)/(1+np.exp(xx))\n",
    "\n",
    "plt.plot(xx,yy)\n",
    "plt.title(\"The logistic, or sigmoid function\")\n",
    "plt.show()"
   ]
