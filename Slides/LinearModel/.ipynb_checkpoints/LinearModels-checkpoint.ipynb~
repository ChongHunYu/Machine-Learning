   "source": [
    "from sklearn.linear_model import Perceptron\n",
    "three = y=='3'\n",
    "one = y=='1'\n",
    "\n",
    "threeOrOne = three+one\n",
    "\n",
    "X_f = X_tf_n[threeOrOne]\n",
    "y_f = y[threeOrOne]\n",
    "\n",
    "clf = Perceptron(tol=1e-3, random_state=0)\n",
    "clf.fit(X_f, y_f)\n",
    "print(clf.score(X_f, y_f))\n",
    "w=clf.coef_[0]"
   ]
